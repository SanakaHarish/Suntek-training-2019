void checksubstring(char *a,char *ch)
{
int p,q,count=0;
for(q=0;q<j;q++)
{
for(p=0;p<i;p++);
{
if(*(a+p)==*(ch+q))
count++;
}
}
if(count==j)    /*if count ==length of sub string*/
printf("present");
else 
printf(" not present");



}

