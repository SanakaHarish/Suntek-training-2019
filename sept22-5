int main()
{
char a[100],ch[50],i,j;
printf("enter string");
scanf("%s",a);
printf("enter sub string");
scanf("%s",ch);
for(i=0;a[i]!='\0';i++);   /*length of sub string*/
for(j=0;ch[j]!='\0';j++);   /*length of sub string*/
check(a,ch);
